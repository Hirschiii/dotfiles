#!/usr/bin/env bash

declare -A my_files
all="School wiki henning kanban journal configs index"
configs="tmux i3 fish qutebrowser nvim dwm st dmenu zsh"

School="Index Inforamtik Deutsch Geschichte Spanisch English Kunst Chemie Mathe Physik Politik Silentium SV Spanisch Technik_AG"

index="$HOME/notes/index.md"

journal="$HOME/notes/Area/Planning/Daily_Notes/$(date +%Y)/$(date +%m)/$(date +%F).md"

kanban="$HOME/notes/Area/Planning/Kanban/Planning.md"

henning="$HOME/notes/Projects/Henning/index.md"

wiki="$HOME/notes/Wiki/index.md"

my_files[Deutsch]="$HOME/notes/School/Deutsch/index.md"
my_files[Geschichte]="$HOME/notes/School/geschichte/index.md"
my_files[Spanisch]="$HOME/notes/School/Spanisch/index.md"
my_files[English]="$HOME/notes/School/English/index.md"
my_files[Chemie]="$HOME/notes/School/Chemie/index.md"
my_files[Kunst]="$HOME/notes/School/Kunst/index.md"
my_files[Mathe]="$HOME/notes/School/Mathe/index.md"
my_files[Physik]="$HOME/notes/School/Physik/index.md"
my_files[Politik]="$HOME/notes/School/Politik/index.md"
my_files[Silentium]="$HOME/notes/School/Silentium/index.md"
my_files[SV]="$HOME/notes/School/SV/index.md"
my_files[Technik_AG]="$HOME/notes/School/Technik_AG/index.md"
my_files[Inforamtik]="$HOME/notes/School/informatik/index.md"
my_files[Index]="$HOME/notes/School/index.md"

my_files[tmux]="$HOME/.config/tmux/tmux.conf"
my_files[i3]="$HOME/.config/i3/config"
my_files[fish]="$HOME/.config/fish/config.fish"
my_files[qutebrowser]="$HOME/.config/qutebrowser/config.py"
my_files[nvim]="$HOME/.config/nvim/init.lua"
my_files[dwm]="$HOME/Projects/suckless/dwm/config.def.h"
my_files[st]="$HOME/Projects/suckless/st/config.def.h"
my_files[dmenu]="$HOME/Projects/suckless/dmenu/config.def.h"
my_files[zsh]="$HOME/.config/zsh/.zshrc"
